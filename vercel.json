
{
    "version": 2,

    "env": {
    },
    "builds": [
      {
        "src": "server/index.js",
        "use": "@vercel/node"
      },{
        "src": "build/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/chatgpt",
        "dest": "server/index.js"
      },
      {
        "src": "/(.+)",
        "dest": "build/$1"
      },
      {
        "src": "/",
        "dest": "public/index.html"
      }
    ]
  }